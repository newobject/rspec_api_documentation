<li class="group">
  <a href="{{ url_prefix }}/{{ file_path }}">
    {{index_number}}. {{ description }}
  </a>

  <ul class="unstyled api">
    {{# examples }}
    <li class="api">
      <a href="{{ url_prefix }}/{{file_path}}">{{index_number}}. {{ description }}</a>
    </li>
    {{/ examples }}
  </ul>

  <ul class="unstyled group">
    {{# children }}
      {{> rspec_api_documentation/mustaches/subgroups}}
    {{/ children }}
  </ul>
</li>
